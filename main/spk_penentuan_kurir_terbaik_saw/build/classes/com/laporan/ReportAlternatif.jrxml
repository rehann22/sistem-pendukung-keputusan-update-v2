	<queryString>
		<![CDATA[SELECT id_kurir, nama, alamat, email, no_telp, tgl_bergabung,
       CONCAT(
           TIMESTAMPDIFF(YEAR, tgl_bergabung, CURDATE()), ' Tahun, ',
           TIMESTAMPDIFF(MONTH, tgl_bergabung, CURDATE()) % 12, ' Bulan'
       ) AS lama_bekerja
FROM tbl_data_kurir;]]>
